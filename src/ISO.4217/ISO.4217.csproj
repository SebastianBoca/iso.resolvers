<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>ISO._4217</RootNamespace>
    <Authors>Anthony Maisak</Authors>
    <Company>Anthony Maisak</Company>
    <Description>Resolver for ISO-4217 codes.</Description>
    <Copyright>Copyright 2019</Copyright>
    <PackageId>ISO.4217.CurrencyCodes</PackageId>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/maisak/iso.resolvers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/maisak/iso.resolvers</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>.net-standard-2.0 iso-4217</PackageTags>
    <Version>1.0.7</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="codes.xml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="ISO.4217.CurrencyCodes.targets" PackagePath="build/ISO.4217.CurrencyCodes.targets" />
    <Content Include="codes.xml" Pack="true" PackagePath="/">
      <PackageCopyToOutput>true</PackageCopyToOutput>
    </Content>
  </ItemGroup>

</Project>
